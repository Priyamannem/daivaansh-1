{
    "headers": [
        {
            "source": "/downloads/(.*)\\.pdf",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/pdf"
                },
                {
                    "key": "Content-Disposition",
                    "value": "inline"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/downloads/(.*)",
            "destination": "/downloads/$1"
        },
        {
            "source": "/((?!downloads|assets|favicon|logo).*)",
            "destination": "/index.html"
        }
    ]
}