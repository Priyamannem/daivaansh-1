{
    "rewrites": [
        {
            "source": "/((?!.*\\.pdf$|assets|favicon|logo).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*).pdf",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/pdf"
                },
                {
                    "key": "Content-Disposition",
                    "value": "inline"
                }
            ]
        }
    ]
}