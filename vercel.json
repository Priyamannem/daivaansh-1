{
    "headers": [
        {
            "source": "/downloads/(.*)\\.pdf",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/pdf"
                },
                {
                    "key": "Content-Disposition",
                    "value": "inline"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31104000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/downloads/:path*",
            "destination": "/downloads/:path*"
        },
        {
            "source": "/((?!downloads/).*)",
            "destination": "/index.html"
        }
    ]
}